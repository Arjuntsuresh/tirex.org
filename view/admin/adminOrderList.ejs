        <section class="content-main">
            <div class="row">
                <div class="col-12">
                    <div class="content-header ">
                        <h2 class="content-title">Order List</h2>
                        <div>
                            <!-- <a href="/admin/category" class="btn btn-primary btn-sm rounded">Create new</a> -->
                        </div>
                    </div>
                </div>


                <div class="card mb-4">
                    <header class="card-header">
                        <div class="row gx-3">
                            <div class="col-lg-4 col-md-6 me-auto">
                                <input type="text" placeholder="Search..." class="form-control">
                            </div>
                            <div class="col-lg-2 col-md-3 col-6">
                                <select class="form-select">
                                    <option>Show all</option>
                                    <option>pending</option>
                                    <option>confirmed</option>
                                    <option>Shipped</option>
                                    <option>Delivered</option>
                                    <option>Canceled</option>
                                </select>
                            </div>
                            <div class="col-lg-2 col-md-3 col-6">
                                <select class="form-select">
                                    <option>Total orders- <%= orderModel.length %>
                                    </option>
                                    <!-- <option>Show 30</option>
                                    <option>Show 40</option> -->
                                </select>
                            </div>
                        </div>
                    </header> <!-- card-header end// -->
                    <div class="card-body">
                        <div class="table-responsive">
                            <% const rowsPerPage = 10; %>
                            <% const totalPages = Math.ceil(orderModel.length / rowsPerPage); %>
                            
                            <% const startIndex = (currentPage - 1) * rowsPerPage; %>
                            <% const endIndex = Math.min(currentPage * rowsPerPage, orderModel.length); %>
                            
                            <table class="table table-white table-striped" style="margin-top: 15px;">
                                <thead>
                                    <tr>
                                        <th>Order</th>
                                        <th>Date</th>
                                        <th>Status</th>
                                        <th>Total</th>
                                        <th style="padding-left: 10%;">Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for (let i = startIndex; i < endIndex; i++) { %>
                                        <tr>
                                            <td>
                                                <% for (let j = 0; j < orderModel[i].product_details.length; j++) { %>
                                                    <p>
                                                        <%= j + i+1 %>. <%= orderModel[i].product_details[j].product_name %>
                                                    </p>
                                                <% } %>
                                            </td>
                                            <td>
                                              
                                                <% const startDate = new Date(orderModel[i].order_date); %>
                                                <% if (!isNaN(startDate)) { %>
                                                    <% const day = startDate.getDate(); %>
                                                    <% const month = startDate.getMonth() + 1; // Months are zero-based, so we add 1 %>
                                                    <% const year = startDate.getFullYear(); %>
                                                    <%= day + '/' + month + '/' + year %>
                                                <% } else { %>
                                                    <%= orderModel[i].order_date %>
                                                <% } %>
                                                
                                            </td>
                                            <td>
                                                <% if (orderModel[i].delivery_status === "confirmed" || orderModel[i].delivery_status === "Shipped" || orderModel[i].delivery_status === "Delivered") { %>
                                                    <span class="badge rounded-pill alert-success" style="font-size: 10px;"><%= orderModel[i].delivery_status %></span>
                                                <% } else { %>
                                                    <span class="badge rounded-pill alert-danger" style="font-size: 10px;"><%= orderModel[i].delivery_status %></span>
                                                <% } %>
                                            </td>
                                            <td>₹<%= orderModel[i].total_price %>/-</td>
                                            <td>
                                                <a href="/admin/orderDetails/?id=<%= orderModel[i]._id %>" class="btn btn-sm btn-info btn-brand">
                                                    <i class="material-icons"></i> Details
                                                </a>
                                                <% if (orderModel[i].delivery_status === 'pending') { %>
                                                    <select class="form-select" data-order-id="<%= orderModel[i]._id %>">
                                                        <option>status</option>
                                                        <option value="Shipping">pending</option>
                                                        <option value="confirmed">confirmed</option>
                                                        <option value="Shipped">Shipped</option>
                                                        <option value="Delivered">Delivered</option>
                                                        <option value="Canceled">Canceled</option>
                                                    </select>
                                                    <p id="selectedValue"></p>
                                                <% } else if (orderModel[i].delivery_status === 'confirmed') { %>
                                                    <select class="form-select" data-order-id="<%= orderModel[i]._id %>">
                                                        <option>status</option>
                                                        <option value="Shipped">Shipped</option>
                                                        <option value="Delivered">Delivered</option>
                                                        <option value="Canceled">Canceled</option>
                                                    </select>
                                                <% } else if (orderModel[i].delivery_status === 'Shipped') { %>
                                                    <select class="form-select" data-order-id="<%= orderModel[i]._id %>">
                                                        <option>status</option>
                                                        <option value="Shipped">Shipped</option>
                                                        <option value="Delivered">Delivered</option>
                                                        <option value="Canceled">Canceled</option>
                                                    </select>
                                                <% } else if (orderModel[i].delivery_status === 'Delivered') { %>
                                                    <select class="form-select" data-order-id="<%= orderModel[i]._id %>">
                                                        <option>status</option>
                                                        <option value="Delivered">Delivered</option>
                                                    </select>
                                                <% } else if (orderModel[i].delivery_status === 'Returned') { %>
                                                    <select class="form-select" data-order-id="<%= orderModel[i]._id %>">
                                                        <option>status</option>
                                                        <option value="Returned">Return</option>
                                                    </select>
                                                <% } else if (orderModel[i].delivery_status === 'canceled') { %>
                                                    <select class="form-select" data-order-id="<%= orderModel[i]._id %>">
                                                        <option>status</option>
                                                        <option value="Canceled">Canceled</option>
                                                    </select>
                                                <% } %>
                                            </td>
                                        </tr>
                                    <% } %>
                                </tbody>
                            </table>
                            
                            <!-- Pagination Control -->
                            <div class="pagination">
                                <% for (let page = 1; page <= totalPages; page++) { %>
                                    <a href="?page=<%= page %>" class="page-link <%= page === currentPage ? 'active' : '' %>"><%= page %></a>
                                <% } %>
                            </div>
                            <!-- table-responsive.// -->
                        </div>
                    </div>
                    <!-- card-body end// -->
                </div>

