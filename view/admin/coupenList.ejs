    <script>
        function checked(id) {
            swal({
                title: "Are you sure?",
                text: "Aru you sure want to block?!",
                icon: "warning",
                buttons: true,
                dangerMode: true,
            })
                .then((willDelete) => {
                    if (willDelete) {
                        console.log("block");
                        location.href = "/admin/coupenblock/?id=" + id
                    } else {
                        swal("Your file is safe!");
                    }
                });
        }

