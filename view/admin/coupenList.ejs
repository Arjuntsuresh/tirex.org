                <div class="col-12">
                    <div class="content-header ">
                        <h2 class="content-title">category List</h2>
                        <div>
                            <a href="/admin/coupen" class="btn btn-primary btn-sm rounded">Create new</a>
                        </div>
                    </div>
                </div>


                <div class="card mb-4">
                    <header class="card-header">
                        <div class="row gx-3">
                           
                          
                            <div class="col-lg-2 col-md-3 col-6">
                                <select class="form-select">
                                    <option>Total coupens- <%= coupens.length %>
                                    </option>
                                    <!-- <option>Show 30</option>
                                    <option>Show 40</option> -->
                                </select>
                            </div>
                        </div>
                    </header> <!-- card-header end// -->
                    <div class="card-body">
                        <div class="table-responsive">
                            <div class="table-responsive">
                                <% const rowsPerPage = 10; %>
                                <% const totalPages = Math.ceil(coupens.length / rowsPerPage); %>
                                
                                <% const currentPage = 1; // Set the current page to the desired value %>
                                <% const startIndex = (currentPage - 1) * rowsPerPage; %>
                                <% const endIndex = Math.min(currentPage * rowsPerPage, coupens.length); %>
                                
                                <table class="table table-white table-striped" style="margin-top: 15px;">
                                    <thead>
                                        <tr>
                                            <th>No</th>
                                            <th>Coupon Code</th>
                                            <th>Status</th>
                                            <th>Starting Date</th>
                                            <th>Expire Date</th>
                                            <th>Coupon Amount</th>
                                            <th class="text-end">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for (let i = startIndex; i < endIndex; i++) { %>
                                            <tr>
                                                <td><%= i + 1 %></td>
                                                <td><%= coupens[i].coupenCode %></td>
                                                <td>
                                                    <% if (coupens[i].is_blocked === false) { %>
                                                        <span class="badge rounded-pill alert-success">Active</span>
                                                    <% } else { %>
                                                        <span class="badge rounded-pill alert-danger">Inactive</span>
                                                    <% } %>
                                                </td>
                                                <td>
                                                  

                                                    <% const startDate = new Date(coupens[i].startDate); %>
                                                           <% if (!isNaN(startDate)) { %>
                                                           <% const day = startDate.getDate(); %>
                                                            <% const month = startDate.getMonth() + 1; // Months are zero-based, so we add 1 %>
                                                             <% const year = startDate.getFullYear(); %>
                                                                <%= day + '/' + month + '/' + year %>
                                                                 <% } else { %>
                                                                  <%= coupens[i].startDate %> 
                                                                      <% } %>
                                                
                                                
                                                
                                                </td>
                                                <td>
                                                   
                                                    <% const endDate = new Date(coupens[i].expireDate); %>
                                                    <% if (!isNaN(endDate)) { %>
                                                        <% const day = endDate.getDate(); %>
                                                        <% const month = endDate.getMonth() + 1; // Months are zero-based, so we add 1 %>
                                                        <% const year = endDate.getFullYear(); %>
                                                        <%= day + '/' + month + '/' + year %>
                                                    <% } else { %>
                                                        <%= coupens[i].expireDate %>
                                                    <% } %>
                                                    
                                                </td>
                                                <td>₹ <%= coupens[i].coupenAmount %>/-</td>
                                                <td class="text-end">
                                                    <% if (coupens[i].is_blocked === false) { %>
                                                        <a onclick="checked('<%= coupens[i]._id %>')">
                                                            <button class="btn btn-sm btn-brand rounded font-sm mt-15">Block</button>
                                                        </a>
                                                    <% } else { %>
                                                        <a href="/admin/coupenUnblock/?id=<%= coupens[i]._id %>">
                                                            <button class="btn btn-sm btn-danger rounded font-sm mt-15">Unblock</button>
                                                        </a>
                                                    <% } %>
                                                </td>
                                            </tr>
                                        <% } %>
                                    </tbody>
                                </table>
                            
                                <!-- Pagination Control -->
                                <div class="pagination">
                                    <% for (let page = 1; page <= totalPages; page++) { %>
                                        <a href="?page=<%= page %>" class="page-link <%= page === currentPage ? 'active' : '' %>"><%= page %></a>
                                    <% } %>
                                </div>
                            </div>
                            
                        </div>
                    </div>
                    
