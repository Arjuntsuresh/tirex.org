    <script>
        new Chart("myChart1", {
            type: "line",
            data: {
                labels: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],
                datasets: [
                    {
                        label: 'Sales (Current Year)',
                        tension: 0.3,
                        fill: true,
                        backgroundColor: 'rgba(4, 209, 130, 0.2)',
                        borderColor: 'rgb(4, 209, 130)',
                        data: <%= JSON.stringify(monthlySalesArray) %>,
                    },
                ]
            },
            options: {
                plugins: {
                    legend: {
                        labels: {
                            usePointStyle: true,
                        },
                    }
                }
            }
        });
    </script>

     <!--user detail-->
     <script>
 
        var barColors = ["red", "green","blue","orange","brown","purple","gray","cyan","teal","lavender","yellow"];
        
        new Chart("myChart3", {
          type: "bar",
          data: {
            labels: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],
            datasets: [{
              backgroundColor: barColors,
              data:  <%= JSON.stringify(usersGrowthArray ) %> 
            }]
          },
          options: {
            legend: {display: false},
            title: {
              display: true,
              text: "User Growth"
            }
          }
        });
        </script>
        
