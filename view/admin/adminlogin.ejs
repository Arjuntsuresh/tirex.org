        <section class="content-main mt-80 mb-80">
            <div class="card mx-auto card-login">
                <div class="card-body">
                    <div class="w-50 p-3 " style="margin-left: 22%;" >
                        <a href="#" class="brand-wrap">
                            <img src="admin-assets/imgs/theme/logo-2.png" class="logo" alt="Evara Dashboard">
                        </a>
                    </div>
                    <h4 class="card-title mb-4" style="margin-left: 36%;" >Sign in</h4>
                    <form method="post" action="/admin/dashboard">
                        <div class="mb-3">
                            <input class="form-control" name="email" id="email" value="" placeholder="Username or email" required
                                type="text">
                            <p id="email_error" style="color: red;"></p>
                        </div> <!-- form-group// -->
                        <div class="mb-3">
                            <input class="form-control" name="password" value="" id="password" placeholder="Password" required
                                type="password">
                            <p id="password_error" style="color: red;"></p>
                        </div> <!-- form-group// -->
                        <div class="mb-3">
                            <!-- <a href="#" class="float-end font-sm text-muted">Forgot password?</a> -->
                            <!-- <label class="form-check">
                                <input type="checkbox" class="form-check-input" checked="">
                                <span class="form-check-label">Remember</span>
                            </label> -->
                        </div> <!-- form-group form-check .// -->
                        <div class="mb-4">
                            <button type="submit" onclick="return validation()" class="btn btn-primary w-100"> Login
                            </button>
                        </div>

                        <% if(typeof message !=="undefined" ){ %>
                            <p
                                style="color: rgb(255, 0, 0); font-family: Arial, Helvetica, sans-serif ; margin-left: 40px; font-weight: 600;">
                                <%= message %>
                            </p>
                            <% } %>
                                <!-- form-group// -->
                    </form>

                </div>
            </div>
        </section>
