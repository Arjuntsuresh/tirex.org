    <script>
        function validateForm() {
            // Get references to form fields and error message elements
            const coupenCode = document.getElementById("coupenCode");
            const startDate = document.getElementById("startDate");
            const endDate = document.getElementById("endDate");
            const coupenAmount = document.getElementById("coupenAmount");
            const coupenCodeError = document.getElementById("coupenCodeErrorMessage");
            const startDateError = document.getElementById("startDateErrorMessage");
            const endDateError = document.getElementById("endDateErrorMessage");
            const coupenAmountError = document.getElementById("coupenAmountErrorMessage");

            // Initialize error messages
            coupenCodeError.textContent = "";
            startDateError.textContent = "";
            endDateError.textContent = "";
            coupenAmountError.textContent = "";

            // Flag to track if the form is valid
            let isValid = true;

            // Validation logic for each field
            if (coupenCode.value === "") {
                coupenCodeError.textContent = "Coupon Code is required";
                isValid = false;
            }

            if (startDate.value === "") {
                startDateError.textContent = "Start Date is required";
                isValid = false;
            }

            if (endDate.value === "") {
                endDateError.textContent = "Expiry Date is required";
                isValid = false;
            } else {
                const today = new Date();
                const selectedDate = new Date(endDate.value);
                if (selectedDate <= today) {
                    endDateError.textContent = "Expiry Date must be in the future";
                    isValid = false;
                }
            }

            if (coupenAmount.value === "") {
                coupenAmountError.textContent = "Coupon Amount is required";
                isValid = false;
            } else if (parseFloat(coupenAmount.value) < 0) {
                coupenAmountError.textContent = "Coupon Amount must be a positive number";
                isValid = false;
            }

            // Show SweetAlert success message if validation is successful
            if (isValid) {
                swal({
                    text: "Form validation successful. Your data has been submitted.",
                    icon: "success"
                });
            } else {
                // Show SweetAlert error message if validation fails
                swal({
                    text: "Form validation failed. Please check the errors and try again.",
                    icon: "error"
                });
            }

            return isValid;
        }
    </script>





