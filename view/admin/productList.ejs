                        </div>
                        <div class="col-md-2 col-6">
                            <input type="date" value="02.05.2021" class="form-control">
                        </div>
                        <div class="col-md-2 col-6">
                            <select id="priceSort" class="form-select">
                                <option value="default" selected>Filter by Price</option>
                                <option value="lowToHigh">Price Low to High</option>
                                <option value="highToLow">Price High to Low</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->



                <div class="table-responsive">
                    <% const rowsPerPage = 5; // Number of items per page %>
                    <% const currentPages = parseInt(currentPage) || 1; // Get the current page from the query parameter %>
                    <% const startIndex = (currentPage - 1) * rowsPerPage; %>
                    <% const endIndex = Math.min(currentPages * rowsPerPage, products.length); %>
                
                    <table class="table">
                        <thead>
                            <tr>
                                <th>Image</th>
                                <th>Product Name</th>
                                <th>Brand</th>
                                <th>Price</th>
                                <th>Status</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for (let i = startIndex; i < endIndex; i++) { %>
                                <tr>
                                    <td>
                                        <img src="admin-assets/imgs/<%= products[i].images[0] %>" class="img-sm img-thumbnail" alt="Item">
                                    </td>
                                    <td>
                                        <%= products[i].productName %>
                                    </td>
                                    <td>
                                        <%= products[i].brandName %>
                                    </td>
                                    <td>
                                        ₹<%= products[i].salePrice %>
                                    </td>
                                    <td>
                                        <% if (products[i].isblocked === false) { %>
                                            <span class="badge rounded-pill alert-success">Active</span>
                                        <% } else { %>
                                            <span class="badge rounded-pill alert-danger">Inactive</span>
                                        <% } %>
                                    </td>
                                    <td>
                                        <a href="/admin/findid/?id=<%= products[i]._id %>" class="btn btn-sm font-sm rounded btn-brand">
                                            <i class="material-icons"></i> Edit
                                        </a>
                                        <% if (products[i].isblocked === false) { %>
                                            <a onclick="checked('<%= products[i]._id %>')" class="btn btn-sm font-sm rounded btn-brand">
                                                <i class="material-icons"></i> Block
                                            </a>
                                        <% } else { %>
                                            <a href="/admin/prounblocked/?id=<%= products[i]._id %>">
                                                <button class="btn btn-sm btn-danger rounded font-sm">Unblock</button>
                                            </a>
                                        <% } %>
                                    </td>
                                </tr>
                            <% } %>
                        </tbody>
                    </table>

