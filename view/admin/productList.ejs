    <script>
        function checked(id) {
            swal({
                title: "Are you sure?",
                text: "Aru you sure want to block?!",
                icon: "warning",
                buttons: true,
                dangerMode: true,
            })
                .then((willDelete) => {
                    if (willDelete) {
                        console.log("block");
                        location.href = "/admin/problocked/?id=" + id
                    } else {
                        swal("Your imaginary file is safe!");
                    }
                });
        }

    </script>
    <script>
        // Add this JavaScript code to your HTML file or an external script

document.getElementById('priceSort').addEventListener('change', function () {
    const selectedOption = this.value;

    // Get the table rows
    const table = document.querySelector('table');
    const tbody = table.querySelector('tbody');
    const rows = Array.from(tbody.querySelectorAll('tr'));

    // Sort the rows based on the selected option
    if (selectedOption === 'lowToHigh') {
        rows.sort((a, b) => {
            const priceA = parseFloat(a.querySelector('td:nth-child(4)').textContent.replace('₹', '').trim());
            const priceB = parseFloat(b.querySelector('td:nth-child(4)').textContent.replace('₹', '').trim());
            return priceA - priceB;
        });
    } else if (selectedOption === 'highToLow') {
        rows.sort((a, b) => {
            const priceA = parseFloat(a.querySelector('td:nth-child(4)').textContent.replace('₹', '').trim());
            const priceB = parseFloat(b.querySelector('td:nth-child(4)').textContent.replace('₹', '').trim());
            return priceB - priceA;
        });
    }else if (selectedOption === 'All') {
        // Restore the original order
        rows.sort((a, b) => {
            const rowIndexA = rows.indexOf(a);
            const rowIndexB = rows.indexOf(b);
            return rowIndexA - rowIndexB;
        });
    }

    // Reorder the table rows
    rows.forEach(row => tbody.appendChild(row));
});

    </script>

    
