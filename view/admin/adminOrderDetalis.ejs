                                                        <% } else { %>
                                                            <span class="badge rounded-pill alert-danger" style="font-size: 10px;"><%= orderModel[i].delivery_status %></span>
                                                        <% } %>
                                                    </td>
                                                    <td style="font-size: 12px;">
                                                        ₹<%= orderModel[i].total_price %>
                                                    </td>
