  <script>
    // Get product list container
    const productList = document.getElementById('product-list');

    // Get the price filter dropdown
    const priceFilter = document.getElementById('price-filter');

    // Add an event listener to handle filter changes
    priceFilter.addEventListener('change', function () {
      // Get selected filter value
      const selectedFilter = priceFilter.value;

      // Get all product items
      const productItems = productList.querySelectorAll('.item');

      // Convert product items to an array for sorting
      const productArray = Array.from(productItems);

      // Sort products based on selected filter
      if (selectedFilter === 'low-to-high') {
        productArray.sort((a, b) => {
          const priceA = parseFloat(a.querySelector('.price').textContent.replace('₹', '').replace('/-', ''));
          const priceB = parseFloat(b.querySelector('.price').textContent.replace('₹', '').replace('/-', ''));
          return priceA - priceB;
        });
      } else if (selectedFilter === 'high-to-low') {
        productArray.sort((a, b) => {
          const priceA = parseFloat(a.querySelector('.price').textContent.replace('₹', '').replace('/-', ''));
          const priceB = parseFloat(b.querySelector('.price').textContent.replace('₹', '').replace('/-', ''));
          return priceB - priceA;
        });
      }

      // Clear the product list
      productList.innerHTML = '';

      // Append the sorted products back to the product list
      productArray.forEach((product) => {
        productList.appendChild(product);
      });
    });
  </script>


