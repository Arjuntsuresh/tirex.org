<script>
    document.addEventListener('DOMContentLoaded', function () {
      const billingForm = document.getElementById('billingForm');
    
      billingForm.addEventListener('submit', function (event) {
        event.preventDefault(); // Prevent the default form submission behavior
    
        let isValid = true;
    
        function validateRequiredField(inputField, errorField) {
          if (inputField.value.trim() === '') {
            errorField.textContent = 'This field is required';
            isValid = false;
          } else {
            errorField.textContent = '';
          }
        }
    
        function validateNameFormat(inputField, errorField) {
          const namePattern = /^[A-Za-z].*$/;
          if (!namePattern.test(inputField.value)) {
            errorField.textContent = 'Name must start with a letter';
            isValid = false;
          } else {
            errorField.textContent = '';
          }
        }
    
        function validateMobile(inputField, errorField) {
          const mobilePattern = /^[0-9]{10}$/;
          if (!mobilePattern.test(inputField.value)) {
            errorField.textContent = 'Please enter a valid 10-digit phone number';
            isValid = false;
          } else {
            errorField.textContent = '';
          }
        }
    
        function validatePincode(inputField, errorField) {
          const pincodePattern = /^[0-9]{6}$/;
          if (!pincodePattern.test(inputField.value)) {
            errorField.textContent = 'Pincode must be a 6-digit number';
            isValid = false;
          } else {
            errorField.textContent = '';
          }
        }
    
        // Custom validation for the address field (e.g., minimum character count)
        function validateAddress(inputField, errorField) {
          if (inputField.value.trim().length < 10) {
            errorField.textContent = 'Address must be at least 10 characters long';
            isValid = false;
          } else {
            errorField.textContent = '';
          }
        }
    
        // Validation for each field
        const firstname = document.getElementById('firstname');
        const lastname = document.getElementById('lastname');
        const mobile = document.getElementById('mobile');
        const address = document.getElementById('address');
        const town = document.getElementById('town');
        const state = document.getElementById('state');
        const pincode = document.getElementById('pincode');
    
        const firstnameError = document.getElementById('firstname_error');
        const lastnameError = document.getElementById('lastname_error');
        const mobileError = document.getElementById('mobile_error');
        const addressError = document.getElementById('address_error');
        const townError = document.getElementById('town_error');
        const stateError = document.getElementById('state_error');
        const pincodeError = document.getElementById('pincode_error');
    
        validateRequiredField(firstname, firstnameError);
        validateRequiredField(lastname, lastnameError);
        validateNameFormat(firstname, firstnameError);
        validateNameFormat(lastname, lastnameError);
        validateMobile(mobile, mobileError);
        validateRequiredField(address, addressError);
        validateRequiredField(town, townError);
        validateRequiredField(state, stateError);
        validatePincode(pincode, pincodeError);
        
        // Additional custom validation for the address field
        validateAddress(address, addressError);
    
        // Display SweetAlert based on form validation status
        if (isValid) {
          // The form is valid, so you can submit the form or perform other actions here.
          // For demonstration, we show a success message.
          Swal.fire({
            icon: 'success',
            title: 'Form Submitted Successfully',
            showConfirmButton: false,
            timer: 1500, // Close the alert after 1.5 seconds
          });
          billingForm.submit(); // Submit the form after displaying the success message
        } else {
          // The form has validation errors, show an error message.
          Swal.fire({
            icon: 'error',
            title: 'Validation Error',
            text: 'Please check the form for errors.',
          });
        }
      });
    });
  </script>
 

<script>
  function validateAddressSelection() {
    const addressRadios = document.querySelectorAll('.address');
    let isAddressSelected = false;

    for (const radio of addressRadios) {
      if (radio.checked) {
        isAddressSelected = true;
        break;
      }
    }

    if (!isAddressSelected) {
      alert('Please select an address.');
      return false; // Prevent form submission
    }

    return true; // Allow form submission
  }
</script>

